    "autoprefixer": "^9.7.6",
    "clean-css": "^4.2.3",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-connect": "^5.7.0",
    "gulp-open": "^3.0.1",
    "less": "^3.11.1",
    "postcss": "^7.0.27",
    "rollup": "^2.4.0",
